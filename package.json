		"eslint": "^8.9.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-vue": "^8.5.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.4",
		"postcss": "^8.4.7",
		"sass": "^1.49.9",
